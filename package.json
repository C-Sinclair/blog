{
  "name": "irrelevant-blog",
  "version": "2.0.0",
  "scripts": {
    "dev": "run-p routify nollup",
    "build": "routify -b && rollup -c && routify export",
    "deploy:netlify": "cd scripts/netlify && npm run deploy",
    "nollup": "nollup -c",
    "routify": "routify"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^13.0.0",
    "@rollup/plugin-node-resolve": "^8.1.0",
    "@rollup/plugin-typescript": "^8.2.0",
    "del": "^5.1.0",
    "husky": "^5.1.3",
    "nollup": "^0.12.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "prettier-plugin-svelte": "^2.2.0",
    "rollup": "^2.18.1",
    "rollup-plugin-copy": "^3.3.0",
    "rollup-plugin-hot": "^0.0.31",
    "rollup-plugin-livereload": "^1.3.0",
    "rollup-plugin-svelte": "^5.2.3",
    "rollup-plugin-svelte-hot": "^0.9.2",
    "rollup-plugin-terser": "^6.1.0",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@rollup/plugin-replace": "^2.3.3",
    "@roxi/routify": "^2.7.3",
    "marked": "^1.1.1",
    "mdsvex": "^0.8.2",
    "postcss-import": "^12.0.1",
    "postcss-preset-env": "^6.7.0",
    "remark-containers": "^1.2.0",
    "remark-slug": "^6.0.0",
    "routify-plugin-frontmatter": "^1.0.1",
    "svelte": "^3.23.2",
    "svelte-icons": "^2.1.0",
    "svelte-preprocess": "^4.0.8"
  },
  "routify": {
    "extensions": "svelte,html,svx,md",
    "dynamicImports": true,
    "routifyDir": ".routify",
    "plugins": {
      "routify-plugin-frontmatter": {}
    }
  }
}
